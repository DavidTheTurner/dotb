{
  "name": "dotb",
  "version": "1.0.0",
  "description": "Toolbelt with various utilities for manipulating data objects",
  "main": "./.webpack/main",
  "config": {
    "forge": "./forge.config.js"
  },
  "scripts": {
    "setup-dev": "type .dev.env > .env",
    "lint": "eslint ./src --ext .ts,.tsx,.js,.jsx",
    "wdio:unit": "wdio run ./unit.wdio.conf.ts",
    "wdio:e2e": "wdio run ./e2e.wdio.conf.ts",
    "wdio": "npm run wdio:unit && npm run wdio:e2e",
    "start": "set ELECTRON_IS_DEV=0 && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "test": "set ELECTRON_IS_DEV=1 && npm run package && npm run wdio",
    "prepare": "husky install"
  },
  "author": "David Turner",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-webpack": "^7.2.0",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@types/css-modules": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@wdio/browser-runner": "^8.27.0",
    "@wdio/cli": "^8.27.1",
    "@wdio/local-runner": "^8.27.0",
    "@wdio/mocha-framework": "^8.27.0",
    "@wdio/spec-reporter": "^8.27.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "electron": "^28.0.0",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "file-loader": "^6.2.0",
    "husky": "^8.0.0",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "wdio-electron-service": "^6.0.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.44.0",
    "@reduxjs/toolkit": "^2.2.1",
    "@types/electron": "^1.6.10",
    "@types/react": "^18.2.44",
    "@types/react-dom": "^18.2.18",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0"
  }
}
